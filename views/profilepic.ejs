<div class="container col-3 position-absolute top-50 start-50 translate-middle">
	<%= typeof msg === 'undefined' ? '' : msg %>
	<form action="/profilepic/update" method="POST" enctype="multipart/form-data">
		<label for="formFileLg" class="form-label">Upload a profilepic</label>
		<input
			class="form-control form-control-lg"
			id="formFileLg"
			type="file"
			name="profilePic"
		/>
		<input
			type="submit"
			value="Submit Pic"
			class="btn btn-primary btn-lg btn-block mt-2"
		/>
	</form>
</div>
