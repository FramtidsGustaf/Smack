<% if(typeof errors != 'undefined') { %> <% errors.forEach(error => {%>
    <div class="alert alert-dismissible alert-warning">
      <strong> <%= error.msg %> </strong>
    </div>
  <% }) %> 
<% } %>

<% if(typeof success_msg != 'undefined') {%>
  <p><%= success_msg %></p>
<% } %>

<% if(typeof error_msg != 'undefined') {%>
    <%= error_msg %> 
<% } %>
